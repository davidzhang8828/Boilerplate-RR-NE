{
  "name": "boilerplate-react-redux-webpack-devserver",
  "version": "1.0.0",
  "description": "Custom-built boilerplate to set up a basic app with React/Redux and Node/Express. Much more barebones than create-react-app. Includes SASS, Jest, and ESLint w/ AirBnB conventions.",
  "scripts": {
    "start": "cross-env NODE_ENV=production node src/server/server.js",
    "prestart": "npm run build",
    "build": "webpack --mode production",
    "dev": "cross-env NODE_ENV=development concurrently \"nodemon src/server/server.js\" \"webpack-dev-server --open\""
  },
  "keywords": [
    "boilerplate",
    "webpack devserver"
  ],
  "author": "David Zhang",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "eslint-plugin-jest": "^23.8.2",
    "express": "^4.17.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "sass": "^1.26.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel": "^6.23.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "html-webpack-plugin": "^4.0.4",
    "jest": "^25.1.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0"
  }
}
